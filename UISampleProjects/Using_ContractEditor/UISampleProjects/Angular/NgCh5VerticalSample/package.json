{
  "name": "ng-ch5-vertical-sample",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:archive": "ch5-cli archive -p ch5-sample-project -P samplesource=Angular/NgCh5VerticalSample -d dist/NgCh5VerticalSample -o dist",
    "build:deploy": "ch5-cli deploy -H hostname -t touchscreen dist/ch5-sample-project.ch5z",
    "build:onestep": "yarn build:prod && yarn build:archive && yarn build:deploy",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/common": "~7.1.0",
